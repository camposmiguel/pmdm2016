    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http://www.flaticon.com"
        android:onClick="abriWebEnNavegador"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http://www.android-app-patterns.com"
        android:onClick="abriWebEnNavegador"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http://www.sgoliver.net"
        android:onClick="abriWebEnNavegador"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http://www.ninjamock.com"
        android:onClick="abriWebEnNavegador"/>
</LinearLayout>
